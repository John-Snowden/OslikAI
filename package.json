{
  "name": "Oslik AI",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios --simulator 'iPhone 14 Pro'",
    "start": "react-native start",
    "tsc": "tsc --noEmit --skipLibCheck",
    "lint": "eslint .",
    "android-release": "cd android && ./gradlew assembleRelease && ./gradlew installRelease && cd ..",
    "clean-ios": "rm -rf node_modules/ && npm cache clean --force && npm install && rm -rf ~/.rncache && rm -rf ./ios/build ",
    "clean-ios-build": "rm -rf ./ios/build",
    "clean-pods": "rm -rf ios/Pods && rm -rf ios/Podfile.lock && cd iOS && pod install && cd ..",
    "clean-android": "cd android && ./gradlew clean && cd ..",
    "clean-bundler": "rm -rf $TMPDIR/react-native-packager-cache-* && rm -rf $TMPDIR/metro-bundler-cache-* && npm cache verify && npm start -- --reset-cache"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.21.0",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/native-stack": "^6.9.13",
    "date-fns": "^2.30.0",
    "mobx": "^6.10.2",
    "mobx-react-lite": "^4.0.4",
    "react": "18.2.0",
    "react-native": "0.72.4",
    "react-native-fast-image": "^8.6.3",
    "react-native-gesture-handler": "^2.13.4",
    "react-native-image-picker": "^7.0.3",
    "react-native-image-zoom-viewer": "^3.0.1",
    "react-native-linear-gradient": "^2.8.2",
    "react-native-manage-external-storage": "^0.1.8",
    "react-native-reanimated": "^3.5.4",
    "react-native-safe-area-context": "^4.7.1",
    "react-native-screens": "^3.24.0",
    "react-native-splash-screen": "^3.3.0",
    "react-native-svg": "^13.13.0",
    "rn-fetch-blob": "^0.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/plugin-proposal-decorators": "^7.23.5",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.11",
    "@tsconfig/react-native": "^3.0.0",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">=16"
  }
}
